#!/usr/bin/env sh
set -eu

SCRIPT="./dist/users.js"

if [ ! -f $SCRIPT ]; then
	echo "$SCRIPT does not exist"
	echo "The compiler needs to be run in order to generate the user script"
	echo ""
	echo "run:"
	echo "    \`yarn build\`"
	echo ""
	echo "in order to run this script"
	exit 1
fi

node $SCRIPT "$@"
