    "cucumber": "0.10.3",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-config-webkom": "^1.3.4",
    "istanbul": "0.4.3",
    "mkdirp": "^0.5.1",
    "mocha": "2.5.2",
    "protractor": "3.3.0",
    "protractor-cucumber-framework": "0.5.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "supertest": "1.2.0",
    "webpack-dev-middleware": "1.6.1"
  }
}
