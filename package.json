{
  "name": "vote",
  "version": "1.3.0",
  "description": "Vote optimizes the election.",
  "main": "app.js",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --config webpack.config.js --mode=production",
    "test": "yarn lint && yarn mocha && yarn protractor",
    "db:reset": "node --no-deprecation test/scripts/db_reset.js",
    "db:seed": "node --no-deprecation test/scripts/db_seed.js",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:yaml",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier '**/*.{js,ts,pug}' --list-different",
    "lint:yaml": "yarn yamllint .drone.yml docker-compose.yml usage.yml deployment/docker-compose.yml",
    "prettier": "prettier '**/*.{js,pug}' --write",
    "mocha": "NODE_ENV=test MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} nyc mocha test/**/*.test.js --exit --timeout 3000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "protractor": "webdriver-manager update --standalone false --versions.chrome 86.0.4240.22 && NODE_ENV=protractor MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} protractor ./features/protractor-conf.js",
    "postinstall": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Webkom <webkom@abakus.no>",
  "repository": {
    "type": "git",
    "url": "git@github.com:webkom/vote.git"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "4.14.167",
    "angular": "1.8.0",
    "angular-animate": "1.7.9",
    "angular-local-storage": "0.7.1",
    "angular-route": "1.7.5",
    "angular1-ui-bootstrap4": "2.4.22",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "commander": "2.19.0",
    "connect-flash": "0.1.1",
    "connect-mongo": "3.2.0",
    "cookie-parser": "1.4.3",
    "crypto-random-string": "1.0.0",
    "css-loader": "2.1.0",
    "csurf": "1.9.0",
    "express": "4.16.4",
    "express-promise-router": "3.0.3",
    "express-session": "1.15.6",
    "file-loader": "3.0.1",
    "lodash": "4.17.19",
    "method-override": "3.0.0",
    "mongoose": "5.8.1",
    "nib": "1.1.2",
    "nyc": "15.1.0",
    "object-assign": "4.1.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-stub": "1.1.1",
    "promptly": "2.1.0",
    "pug": "2.0.4",
    "qr-scanner": "1.1.1",
    "qrcode": "1.3.4",
    "raven": "2.6.4",
    "redis": "3.0.2",
    "redlock": "3.1.2",
    "serve-favicon": "2.5.0",
    "socket.io": "2.2.0",
    "style-loader": "0.23.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "webpack": "4.28.4",
    "webpack-cli": "4.0.0",
    "yaml": "1.3.2"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "1.10.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.0.9",
    "cucumber": "0.10.3",
    "eslint": "5.14.1",
    "eslint-config-prettier": "6.7.0",
    "mkdirp": "^0.5.1",
    "mocha": "6.2.3",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "protractor-cucumber-framework": "0.6.0",
    "sinon": "7.2.2",
    "sinon-chai": "3.3.0",
    "supertest": "3.3.0",
    "webpack-dev-middleware": "3.5.0",
    "yaml-lint": "1.2.4"
  }
}
