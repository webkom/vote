{
  "name": "vote",
  "version": "2.1.0",
  "description": "Vote optimizes the election.",
  "main": "app.js",
  "scripts": {
    "dev": "webpack --config webpack.server.js --mode=development && node dist/server.js",
    "start": "node dist/server.js",
    "build": "webpack --config webpack.server.js --mode=production && webpack --config webpack.config.js --mode=production",
    "test": "yarn lint && yarn mocha && yarn protractor",
    "db:reset": "node --no-deprecation test/scripts/db_reset.js",
    "db:seed": "node --no-deprecation test/scripts/db_seed.js",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:yaml",
    "lint:eslint": "eslint . --ignore-path .gitignore --ignore-pattern 'app/algorithms/*.js'",
    "lint:prettier": "prettier '**/*.{js,ts,pug}' --list-different",
    "lint:yaml": "yarn yamllint docker-compose.yml usage.yml deployment/docker-compose.yml",
    "prettier": "prettier '**/*.{js,ts,pug}' --write",
    "mocha": "NODE_ENV=test MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} nyc mocha -r test/babel-register.js test/**/*.test.js --exit --timeout 30000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "protractor": "webdriver-manager update --standalone false --versions.chrome 96.0.4664.110 && NODE_ENV=protractor MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} protractor ./features/protractor-conf.js",
    "postinstall": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Webkom <webkom@abakus.no>",
  "repository": {
    "type": "git",
    "url": "git@github.com:webkom/vote.git"
  },
  "license": "MIT",
