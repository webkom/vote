{
  "name": "vote",
  "version": "1.0.0",
  "description": "Vote optimizes the election.",
  "main": "app.js",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --config webpack/prod.config.js",
    "test": "npm run lint && npm run mocha && npm run protractor",
    "lint": "eslint . --ignore-path .gitignore",
    "mocha": "NODE_ENV=test MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} istanbul cover _mocha $(find test -name '*.test.js') && istanbul report cobertura",
    "protractor": "webdriver-manager update --standalone false && NODE_ENV=protractor MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} protractor ./features/protractor-conf.js",
    "postinstall": "npm run build"
  },
  "author": "Webkom <webkom@abakus.no>",
  "repository": {
    "type": "git",
    "url": "git@github.com:webkom/vote.git"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.0",
    "angular-local-storage": "0.2.7",
    "angular-route": "1.5.3",
    "angular-ui-bootstrap": "1.2.5",
    "bcrypt": "0.8.5",
    "bluebird": "3.3.3",
    "body-parser": "1.15.0",
    "bootstrap": "~3.3.2",
    "chalk": "1.1.1",
    "connect-flash": "0.1.1",
    "connect-mongo": "1.1.0",
    "cookie-parser": "1.4.1",
    "css-loader": "0.23.1",
    "csurf": "1.8.3",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "extract-text-webpack-plugin": "1.0.1",
    "font-awesome-webpack": "0.0.4",
    "jade": "^1.11.0",
    "lodash": "4.6.1",
    "method-override": "2.3.5",
    "mongoose": "4.4.7",
    "nib": "1.1.0",
    "object-assign": "4.0.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-stub": "1.1.1",
    "promptly": "1.0.0",
    "raven": "^0.10.0",
    "serve-favicon": "2.3.0",
    "socket.io": "~1.4.5",
