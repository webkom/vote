{
  "name": "vote",
  "version": "1.0.0",
  "description": "Vote optimizes the election.",
  "main": "app.js",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --config webpack/prod.config.js",
    "test": "yarn lint && yarn mocha && yarn protractor",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier '**/*.js' --list-different --ignore-path .gitignore",
    "prettier": "prettier '**/*.js' --write",
    "mocha": "NODE_ENV=test MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} nyc mocha test/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "protractor": "webdriver-manager update --standalone false && NODE_ENV=protractor MONGO_URL=${MONGO_URL:-'mongodb://localhost:27017/vote-test'} protractor ./features/protractor-conf.js",
    "postinstall": "yarn build"
  },
  "prettier": {
    "singleQuote": true
  },
  "author": "Webkom <webkom@abakus.no>",
  "repository": {
    "type": "git",
    "url": "git@github.com:webkom/vote.git"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.7.5",
    "angular-local-storage": "0.7.1",
    "angular-route": "1.7.5",
    "angular-ui-bootstrap": "2.5.6",
    "bcrypt": "3.0.3",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "bootstrap": "4.2.1",
    "chalk": "2.4.2",
    "commander": "2.19.0",
    "connect-flash": "0.1.1",
    "connect-mongo": "2.0.3",
    "cookie-parser": "1.4.3",
    "css-loader": "2.1.0",
    "csurf": "1.9.0",
    "express": "4.16.4",
    "express-promise-router": "3.0.3",
    "express-session": "1.15.6",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "font-awesome-webpack": "0.0.4",
    "lodash": "4.17.11",
    "method-override": "3.0.0",
    "mongoose": "5.4.3",
    "nib": "1.1.2",
    "nyc": "13.1.0",
    "object-assign": "4.1.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-stub": "1.1.1",
    "prompt": "1.0.0",
    "pug": "2.0.3",
    "raven": "2.6.4",
    "serve-favicon": "2.5.0",
    "socket.io": "2.2.0",
    "style-loader": "0.23.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "webpack": "4.28.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.0.2",
    "cucumber": "5.1.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "5.2.0",
    "prettier": "1.15.3",
    "protractor": "5.4.2",
    "protractor-cucumber-framework": "6.1.1",
    "sinon": "7.2.2",
    "sinon-chai": "3.3.0",
    "supertest": "3.3.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.5.0"
  }
}
