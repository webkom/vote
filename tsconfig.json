{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es6",
    "outDir": "./dist",
    "module": "ESNext",
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": false
  },
  "exclude": ["node_modules", "public"]
}
