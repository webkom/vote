    "sourceMap": true,
    "noImplicitAny": true,
    "target": "esnext",
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "module": "esnext"
  },
  "include": ["src"]
}
